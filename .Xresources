! Set DPI for HiDPI screen
Xft.dpi: 144

! text autohinting
Xft.autohint: 0

! one of: lcddefault, lcdlight, lcdlegacy, lcdnone
Xft.lcdfilter: lcddefault

! text hinting style, one of: hintnone, hintslight, hintmedium, hintfull
Xft.hintstyle: hintfull

! text hinting
Xft.hinting: 1

! text antialiasing
Xft.antialias: 1

! text subpixel rendering, one of: none, rgb, bgr, vrgb, vbgr
Xft.rgba: rgb

! xterm font
xterm*faceName: Hack
xterm*faceSize: 9

! special
*.foreground:   #e8eaed
*.background:   #000000
*.cursorColor:  #e8eaed

! black
*.color0:       #2b2b2b
*.color8:       #5a647e

! red
*.color1:       #da4939
*.color9:       #da4939

! green
*.color2:       #a5c261
*.color10:      #a5c261

! yellow
*.color3:       #ffc66d
*.color11:      #ffc66d

! blue
*.color4:       #6d9cbe
*.color12:      #6d9cbe

! magenta
*.color5:       #b6b3eb
*.color13:      #b6b3eb

! cyan
*.color6:       #519f50
*.color14:      #519f50

! white
*.color7:       #e6e1dc
*.color15:      #f9f7f3

! xterm make alt normal
xterm*metaSendsEscape: true
